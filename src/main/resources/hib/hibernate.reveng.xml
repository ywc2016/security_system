<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<type-mapping>
		<sql-type jdbc-type="CHAR" hibernate-type='string'
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type='string'
			not-null="false">
		</sql-type>

		<sql-type jdbc-type="BIGINT" hibernate-type='java.lang.Long'>
		</sql-type>
		<sql-type jdbc-type="TINYINT"
			hibernate-type="java.lang.Integer">
		</sql-type>
	</type-mapping>
	<table-filter match-name="e_document"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="e_document_backup"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="e_document_version_management"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="inf_staff"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_authority"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_authority_resource"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_resource"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_role"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_role_authority"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_user"
		match-catalog="security_system">
	</table-filter>
	<table-filter match-name="sys_user_role"
		match-catalog="security_system">
	</table-filter>
</hibernate-reverse-engineering>