<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.buaa.sem.po.InfStaff" table="inf_staff" schema="amc">
        <id name="id" column="id"/>
        <property name="number" column="number"/>
        <property name="infDepartmentId" column="inf_department_id"/>
        <property name="name" column="name"/>
        <property name="gender" column="gender"/>
        <property name="birthday" column="birthday"/>
        <property name="phone" column="phone"/>
        <property name="email" column="email"/>
        <property name="title" column="title"/>
        <property name="date" column="date"/>
        <property name="diploma" column="diploma"/>
        <property name="graduateSchool" column="graduate_school"/>
        <property name="graduateTime" column="graduate_time"/>
        <property name="majority" column="majority"/>
        <property name="working" column="working"/>
        <property name="certificate" column="certificate"/>
        <property name="professionTitle" column="profession_title"/>
        <property name="lastCompany" column="last_company"/>
        <property name="insuranceExperience" column="insurance_experience"/>
        <property name="insuranceBrokerageExperience" column="insurance_brokerage_experience"/>
        <property name="enabled" column="enabled"/>
        <property name="userRelate" column="user_relate"/>
        <property name="businessExpertise" column="business_expertise"/>
    </class>
</hibernate-mapping>