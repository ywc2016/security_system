<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.buaa.sem.po.StoGoods" table="sto_goods" schema="amc">
        <id name="id" column="id"/>
        <property name="number" column="number"/>
        <property name="name" column="name"/>
        <property name="inventory" column="inventory"/>
        <property name="situation" column="situation"/>
        <property name="need" column="need"/>
        <property name="suggestNumber" column="suggest_number"/>
        <property name="salseNumber" column="salse_number"/>
        <property name="salsePrice" column="salse_price"/>
        <property name="location" column="location"/>
        <property name="pictureUrl" column="picture_url"/>
    </class>
</hibernate-mapping>