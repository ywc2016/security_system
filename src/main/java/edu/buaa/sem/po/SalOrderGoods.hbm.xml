<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.buaa.sem.po.SalOrderGoods" table="sal_order_goods" schema="amc">
        <id name="id" column="id"/>
        <property name="orderId" column="order_id"/>
        <property name="goodsId" column="goods_id"/>
        <property name="needNumber" column="need_number"/>
        <property name="suppliedNumber" column="supplied_number"/>
        <property name="availableNumber" column="available_number"/>
        <property name="status" column="status"/>
        <property name="unitPrice" column="unit_price"/>
        <property name="isReturn" column="is_return"/>
    </class>
</hibernate-mapping>